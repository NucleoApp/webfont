License - nucleoapp.com/license/

-------------------------------- */
@font-face {
  font-family: '{{fontName}}';
  src: url('../fonts/{{fontName}}.eot');
  src: url('../fonts/{{fontName}}.eot') format('embedded-opentype'), url('../fonts/{{fontName}}.woff2') format('woff2'), url('../fonts/{{fontName}}.woff') format('woff'), url('../fonts/{{fontName}}.ttf') format('truetype'), url('../fonts/{{fontName}}.svg') format('svg');
  font-weight: normal;
  font-style: normal;
}
/*------------------------
	base class definition
-------------------------*/
.{{ className }}-icon {
  display: inline-block;
  font: normal normal normal 14px/1 '{{fontName}}';
  font-size: inherit;
  speak: none;
  text-transform: none;
  /* Better Font Rendering */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
/*------------------------
  change icon size
-------------------------*/
.{{ className }}-icon.lg {
  font-size: 1.33333333em;
  vertical-align: -16%;
}
.{{ className }}-icon.x2 {
  font-size: 2em;
}
.{{ className }}-icon.x3 {
  font-size: 3em;
}
/*----------------------------------
  add a square/circle background
-----------------------------------*/
.{{ className }}-icon.square,
.{{ className }}-icon.circle {
  padding: 0.33333333em;
  vertical-align: -16%;
  background-color: #eee;
}
.{{ className }}-icon.circle {
  border-radius: 50%;
}
/*------------------------
  list icons
-------------------------*/
.{{ className }}-icon-ul {
  padding-left: 0;
  margin-left: 2.14285714em;
  list-style-type: none;
}
.{{ className }}-icon-ul > li {
  position: relative;
}
.{{ className }}-icon-ul > li > .{{ className }}-icon {
  position: absolute;
  left: -1.57142857em;
  top: 0.14285714em;
  text-align: center;
}
.{{ className }}-icon-ul > li > .{{ className }}-icon.lg {
  top: 0;
  left: -1.35714286em;
}
.{{ className }}-icon-ul > li > .{{ className }}-icon.circle,
.{{ className }}-icon-ul > li > .{{ className }}-icon.square {
  top: -0.19047619em;
  left: -1.9047619em;
}
/*------------------------
  spinning icons
-------------------------*/
.{{ className }}-icon.spin {
  -webkit-animation: {{ className }}-icon-spin 2s infinite linear;
  -moz-animation: {{ className }}-icon-spin 2s infinite linear;
  animation: {{ className }}-icon-spin 2s infinite linear;
}
@-webkit-keyframes {{ className }}-icon-spin {
  0% {
    -webkit-transform: rotate(0deg);
  }
  100% {
    -webkit-transform: rotate(360deg);
  }
}
@-moz-keyframes {{ className }}-icon-spin {
  0% {
    -moz-transform: rotate(0deg);
  }
  100% {
    -moz-transform: rotate(360deg);
  }
}
@keyframes {{ className }}-icon-spin {
  0% {
    -webkit-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -ms-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
  }
  100% {
    -webkit-transform: rotate(360deg);
    -moz-transform: rotate(360deg);
    -ms-transform: rotate(360deg);
    -o-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}
/*------------------------
  rotated/flipped icons
-------------------------*/
.{{ className }}-icon.rotate-90 {
  filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=1);
  -webkit-transform: rotate(90deg);
  -moz-transform: rotate(90deg);
  -ms-transform: rotate(90deg);
  -o-transform: rotate(90deg);
  transform: rotate(90deg);
}
.{{ className }}-icon.rotate-180 {
  filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=2);
  -webkit-transform: rotate(180deg);
  -moz-transform: rotate(180deg);
  -ms-transform: rotate(180deg);
  -o-transform: rotate(180deg);
  transform: rotate(180deg);
}
.{{ className }}-icon.rotate-270 {
  filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=3);
  -webkit-transform: rotate(270deg);
  -moz-transform: rotate(270deg);
  -ms-transform: rotate(270deg);
  -o-transform: rotate(270deg);
  transform: rotate(270deg);
}
.{{ className }}-icon.flip-y {
  filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=0);
  -webkit-transform: scale(-1, 1);
  -moz-transform: scale(-1, 1);
  -ms-transform: scale(-1, 1);
  -o-transform: scale(-1, 1);
  transform: scale(-1, 1);
}
.{{ className }}-icon.flip-x {
  filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=2);
  -webkit-transform: scale(1, -1);
  -moz-transform: scale(1, -1);
  -ms-transform: scale(1, -1);
  -o-transform: scale(1, -1);
  transform: scale(1, -1);
}
/*------------------------
	font icons
-------------------------*/
/* all icon font classes list here */
{% for glyph in glyphs%}
.{{ className }}-{{ glyph.name }}::before {
    content: "\{{ glyph.unicode[0].charCodeAt(0).toString(16) }}";
}
{% endfor %}
